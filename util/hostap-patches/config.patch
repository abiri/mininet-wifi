--- hostapd/defconfig
+++ hostapd/defconfig
@@ -28,7 +28,7 @@ CONFIG_DRIVER_NL80211=y
 #CONFIG_LIBNL20=y

 # Use libnl 3.2 libraries (if this is selected, CONFIG_LIBNL20 is ignored)
